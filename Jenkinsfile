pipeline {
	agent any
	stages {	
		stage("build"){
			steps {
				echo "building..."
				echo "testing auto trigger..."
				echo "does not work..."
				echo "hello world"
			}
			
		}
		stage("test") {
			steps {
				echo "testing..."
				echo "OK tested..."
			}
		}
		stage("deploy") {
			steps {
				echo "deploying..."
				echo "deployment done..."
			}
		}
	}
}
